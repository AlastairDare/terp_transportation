{
    "doctype": "DocType",
    "name": "Toll Page Result",
    "module": "Transportation",
    "custom": 1,
    "autoname": "format:PAGE-{parent_document}-{page_number}-{#####}",
    "fields": [
        {
            "fieldname": "parent_document",
            "fieldtype": "Link",
            "label": "Parent Document",
            "options": "Toll Capture",
            "reqd": 1
        },
        {
            "fieldname": "page_number",
            "fieldtype": "Int",
            "label": "Page Number",
            "reqd": 1
        },
        {
            "fieldname": "base64_image",
            "fieldtype": "Long Text",
            "label": "Base64 Image"
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "Unprocessed/nProcessed/nError",
            "default": "Unprocessed",
            "reqd": 1
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
    "track_changes": 1
}